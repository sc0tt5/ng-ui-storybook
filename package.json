{
    "name": "ng-ui-storybook",
    "version": "0.0.0",
    "scripts": {
        "ng": "ng",
        "start": "npm run story",
        "build": "ng build",
        "build:production": "npm run clean && ng build --prod",
        "clean": "rimraf dist",
        "test": "ng test",
        "lint": "ng lint",
        "e2e": "ng e2e",
        "story": "start-storybook -p 6006",
        "story:build": "build-storybook",
        "lib:version": "npm version patch --prefix projects/lib-shared",
        "lib:start": "npm run clean && ng build lib-shared  --watch",
        "lib:build": "npm run clean && ng build lib-shared",
        "lib:pack": "npm run lib:build && cd dist/lib-shared && npm pack",
        "lib:install": "npm install dist/lib-shared",
        "lib": "npm run lib:pack && npm run lib:install"
    },
    "private": true,
    "dependencies": {
        "@angular/animations": "~8.1.1",
        "@angular/common": "~8.1.1",
        "@angular/compiler": "~8.1.1",
        "@angular/core": "~8.1.1",
        "@angular/forms": "~8.1.1",
        "@angular/platform-browser": "~8.1.1",
        "@angular/platform-browser-dynamic": "~8.1.1",
        "@angular/router": "~8.1.1",
        "lib-shared": "file:dist/lib-shared",
        "rxjs": "~6.4.0",
        "tslib": "^1.9.0",
        "zone.js": "~0.9.1"
    },
    "devDependencies": {
        "@angular-devkit/build-angular": "~0.801.2",
        "@angular-devkit/build-ng-packagr": "~0.801.2",
        "@angular/cli": "~8.1.1",
        "@angular/compiler-cli": "~8.1.1",
        "@angular/language-service": "~8.1.1",
        "@babel/core": "^7.5.5",
        "@storybook/addon-actions": "^5.1.9",
        "@storybook/addon-knobs": "^5.1.9",
        "@storybook/addon-links": "^5.1.9",
        "@storybook/addon-notes": "^5.1.9",
        "@storybook/addon-viewport": "^5.1.10",
        "@storybook/addons": "^5.1.9",
        "@storybook/angular": "^5.1.9",
        "@storybook/theming": "^5.1.10",
        "@types/jasmine": "~3.3.8",
        "@types/jasminewd2": "~2.0.3",
        "@types/node": "~8.9.4",
        "@types/storybook__addon-knobs": "^5.0.3",
        "awesome-typescript-loader": "^5.2.1",
        "babel-loader": "^8.0.6",
        "codelyzer": "^5.0.0",
        "css-loader": "^3.1.0",
        "jasmine-core": "~3.4.0",
        "jasmine-spec-reporter": "~4.2.1",
        "karma": "~4.1.0",
        "karma-chrome-launcher": "~2.2.0",
        "karma-coverage-istanbul-reporter": "~2.0.1",
        "karma-jasmine": "~2.0.1",
        "karma-jasmine-html-reporter": "^1.4.0",
        "ng-packagr": "^5.1.0",
        "node-sass": "^7.0.0",
        "protractor": "~5.4.0",
        "rimraf": "^2.6.3",
        "sass-loader": "^7.1.0",
        "style-loader": "^0.23.1",
        "ts-node": "~7.0.0",
        "tsickle": "^0.35.0",
        "tslint": "~5.15.0",
        "typescript": "~3.4.3"
    }
}
